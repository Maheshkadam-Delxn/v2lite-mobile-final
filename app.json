{
  "expo": {
    "name": "v2-lite",
    "slug": "v2-lite",
    "version": "1.0.0",
    "scheme": "v2lite",
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-asset"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.maheshkadam.v2lite"
    },
    "extra": {
      "eas": {
        "projectId": "8f6d54b8-71ad-4fa6-9b6f-900f6f013f69"
      }
    }
  }
}
